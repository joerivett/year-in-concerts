<!DOCTYPE html>
<html>
  <head>
    <title>2017 In Concerts</title>
    <%= csrf_meta_tags %>

    <script
      src="https://code.jquery.com/jquery-3.2.1.min.js"
      integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
      crossorigin="anonymous"></script>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, width=device-width" />

    <link href="https://fonts.googleapis.com/css?family=PT+Sans|Rock+Salt" rel="stylesheet" />

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112473045-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-112473045-1');
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
